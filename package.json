{
  "name": "@kwilax/react-native-auth-screens",
  "version": "1.0.0",
  "description": "A Zero Dependency Authentication Screen UI for React Native & Expo",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "rm -rf dist/ && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc",
    "build:cjs": "tsc --module CommonJS --outDir dist/cjs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/kwilax-core/React-Native-Auth-Screens.git"
  },
  "keywords": [
    "React-Native",
    "Auth",
    "Authentication",
    "Screen",
    "UI"
  ],
  "author": "sh3riff",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/kwilax-core/React-Native-Auth-Screens/issues"
  },
  "homepage": "https://github.com/kwilax-core/React-Native-Auth-Screens#readme",
  "peerDependencies": {
    "react": "*",
    "react-native": "*"
  },
  "devDependencies": {
    "react": "18.0.0",
    "react-native": "0.69.2",
    "typescript": "^4.4.4"
  }
}
